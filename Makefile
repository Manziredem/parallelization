print:print.c
	gcc print.c -o print

hello:hello.c
	gcc hello.c -o hello

a1:a1.c
	gcc a1.c -o a1

variablesEx3:variablesEx3.c
	gcc variablesEx3.c -o variablesEx3

datatypes:datatypes.c
	gcc datatypes.c -o datatypes

constants:constants.c
	gcc constants.c -o constants

ifStat:ifStat.c
	gcc ifStat.c -o ifStat